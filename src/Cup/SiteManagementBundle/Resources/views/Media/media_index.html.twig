{# empty Twig template #}
{% extends 'page.html.twig' %}
{% block body %}
  <section class="section clearfix">
<div class="container">
 <div class="row">
                <div class="col-lg-9">
                <div class="small-gap"></div>
                <h2>Medias List</h2>
                </div>
                <div class="col-lg-3">
                      <div class="small-gap"></div>
                    <a href="{{ path('cup_site_management_media_add') }}" class="btn btn-primary">Add Media </a>
                </div>
              </div>  
    
    
    <div class="row">
   <div class="col-lg-12">
    <table class="table table-striped table-responsive">
 

<thead> 
<tr> 
	<th>SR</th> 
	
	<th>Media Name</th>

	<th>News Subject</th>
	
{#}	<th>News Description  </th> {#}
	
	<th>News Title  </th>
	
	<th>News Link  </th>
	

	<th>Media Logo</th>
	
        
</tr> 
</thead> 


<tbody>
    {% for media in medias %}
<tr> 
	<td>{{ loop.index }}</td> 

	<td>{{ media.mediaName }} </td>
	<td>{{ media.newsSubject }} </td>
	{#}<td> {{ media.newsDescription}} </td>{#}
	<td> {{ media.newsTitle}} </td>
	<td> {{ media.newsLink}} </td>
	<td>{{ media.mediaLogo}} </td>
	{#}<td> {% if client.active %} yes {% else %} No {% endif %}</td> {#}
	
        <td>
            <a href="{{ path('cup_site_management_media_edit', {'id': media.id})}}" > Edit </a>
            <a href="{{ path('cup_site_management_media_delete', {'id': media.id})}}"> Delete </a>
        </td> 
</tr> 
{% endfor %} 
</tbody> 

</table>
</div>
</div>
</div>
</section>

{% endblock %}