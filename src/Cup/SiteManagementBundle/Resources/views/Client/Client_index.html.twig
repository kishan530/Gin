{# empty Twig template #}
{% extends 'page.html.twig' %}
{% block body %}
  <section class="section clearfix">
<div class="container">
 <div class="row">
                <div class="col-lg-9">
                <div class="small-gap"></div>
                <h2>Clients List</h2>
                </div>
                <div class="col-lg-3">
                      <div class="small-gap"></div>
                    <a href="{{ path('cup_site_management_campaign_client_add') }}" class="btn btn-primary">Add New</a>
                </div>
              </div>  
    
    
    <div class="row">
   <div class="col-lg-12">
    <table class="table table-striped table-responsive">
 

<thead> 
<tr> 
	<th>SR</th> 
	<th>Testimonial Title</th>
	{#}
	<th>Testimonial Description</th>
	{#}
	<th>Client Name</th>
	<th>StoryTitle</th>
	{#}
	<th>StoryDescriptionth</th>
	{#}
	<th>StoryImage</th>
	<th>Active</th> 
	<th></th>
        
</tr> 
</thead> 


<tbody>
    {% for client in clients %}
<tr> 
	<td>{{ loop.index }}</td> 

	<td>{{ client.subject }} </td>
	{#}
	<td>{{ client.description }} </td>
	{#}
	<td>{{ client.clientName }} </td>
	<td> {{ client.storyTitle}} </td>
	{#}
	<td>{{ client.storyDescription}}</td>
	{#}
	<td>{{ client.storyImage}} </td>
	<td> {% if client.active %} yes {% else %} No {% endif %}</td>
	
        <td>
           {#} <a href="{{ path('cup_site_management_campaign_client_add')}}" > Create </a>{#}
            {#}
            <a href="{{ path('cup_site_management_campaign_client_view', {'id': client.id})}}" > View </a>
            {#}
			<a href="{{ path('cup_site_management_campaign_client_edit', {'id': client.id})}}" > Edit </a>
            <a href="{{ path('cup_site_management_campaign_client_delete', {'id': client.id})}}"> Delete </a>
        </td> 
</tr> 
{% endfor %} 
</tbody> 

</table>
</div>
</div>
</div>
</section>

{% endblock %}