{% extends 'page.html.twig' %}
{% block javascripts %}

{% endblock %}
{% block body %}

    <section class="section clearfix">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                <div class="small-gap"></div>
                <h2>Contact List</h2>
                </div>
                <div id="fullwidth" class="col-sm-12">
                    <div class="col-lg-10">
                        <table id="result" class="table table-bordered col-lg-12 col-sm-12">
                           {% if contacts|length>0 %}	
                                  <tr class="blue">
                                        <th>Sl.No </th>
                                        <th>Name</th>
                                        <th>Mobile</th>
                                        <th>Email</th>
                                         <th>City</th>
                                         <th>Number of Employees</th>
                                         <th>Number of Cups</th>
                                         <th>Advertisetype</th>                                                                    
                                        <th> Message </th> 
                                    </tr>	
                            {% endif %}			

                             {% for contact in contacts if contacts|length>0 %}
                                <tr>
                                    <td>{{ loop.index }}</td>
                                    <td>{{ contact.name }}</td> 
                                    <td>{{ contact.mobile }}</td> 
                                   <td>{{ contact.email }}</td>
                                     <td>{{ contact.email }}</td> 
                                     <td>{{ contact.city }}</td>
                                     <td>{{ contact.numberOfCups }}</td>
                                     <td>{{ contact.numberOfEmployees }}</td> 
                                     <td>{{ contact.advertiseType }}</td>                                   
                                    <td>{{ contact.message }}</td>

                                </tr>
                            {% endfor %}
                        </table>
                    </div>
                </div>
            </div>

        </div>
</section>
{% endblock %}